<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankbasic2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FastPay"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Transact"></activity>
        <activity android:name=".DisplayAccount" />
        <activity android:name=".AccountList" />
        <activity android:name=".TransactionHistory" />
        <activity android:name=".TransactionSuccessful"></activity>
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>